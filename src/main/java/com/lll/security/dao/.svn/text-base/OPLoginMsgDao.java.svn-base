package com.sitech.security.dao;

import org.mybatis.spring.support.SqlSessionDaoSupport;

import com.sitech.security.entity.OPLoginMsg;

public class OPLoginMsgDao extends SqlSessionDaoSupport{
	public OPLoginMsg findLoginMsgByLoginNo(String loginNo){
		return this.getSqlSession().selectOne("OPLoginMsgMapper.getLoginMsgByLoginNo", loginNo);
	}
}
